Subject: [PATCH 4/4] FEAT:Add isolcpus=1 in kernel command

---
 arch/arm64/configs/myd_lma35d1_defconfig | 3 +++
 1 file changed, 3 insertions(+)

diff --git a/arch/arm64/configs/myd_lma35d1_defconfig b/arch/arm64/configs/myd_lma35d1_defconfig
index c9dcfec57..aa18e642a 100755
--- a/arch/arm64/configs/myd_lma35d1_defconfig
+++ b/arch/arm64/configs/myd_lma35d1_defconfig
@@ -585,3 +585,6 @@ CONFIG_EEPROM_AT24=y
 CONFIG_GPIO_WATCHDOG=y
 CONFIG_CONSOLE_LOGLEVEL_DEFAULT=1
 CONFIG_PREEMPT_RT=y
+# CONFIG_INITRAMFS_FORCE is not set
+CONFIG_CMDLINE="root=/dev/mmcblk1p1 rootfstype=ext4 rw rootwait isolcpus=1 console=ttyS0,115200n8 rdinit=/sbin/init mem=248M"
+CONFIG_CMDLINE_FORCE=y
